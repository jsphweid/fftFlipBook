{
  "name": "fft-flip-book",
  "version": "0.1.0",
  "description": "A flip book to explore the FFTs of audio visually.",
  "main": "dist/package/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "build": "npm run clean && npm run build:artifacts && npm run build:package",
    "build:artifacts": "webpack --config webpack.prod.config.js",
    "build:package": "tsc && npm run build:sass",
    "build:sass": "node-sass src/styles.scss dist/package/styles.css",
    "clean": "rm -rf dist",
    "lint": "tslint -p ./tsconfig.json",
    "analyze:size": "NODE_ENV=fakerun webpack --config webpack.library.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.0.13",
    "@types/react-dom": "^16.0.1",
    "@types/webpack": "^3.0.13",
    "@types/webpack-env": "^1.13.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.2.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  }
}
